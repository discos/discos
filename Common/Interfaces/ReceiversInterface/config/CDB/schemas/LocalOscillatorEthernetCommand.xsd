<?xml version="1.0" encoding="ISO-8859-1"?>
<xs:schema 
  targetNamespace="urn:schemas-cosylab-com:LocalOscillatorEthernetCommand:1.0"
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns="urn:schemas-cosylab-com:LocalOscillatorEthernetCommand:1.0"
  xmlns:cdb="urn:schemas-cosylab-com:CDB:1.0" 
  xmlns:baci="urn:schemas-cosylab-com:BACI:1.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:import namespace="urn:schemas-cosylab-com:CDB:1.0" schemaLocation="CDB.xsd"/>
  <xs:import namespace="urn:schemas-cosylab-com:BACI:1.0" schemaLocation="BACI.xsd"/>

  <xs:complexType name="LocalOscillatorEthCommandType">
    <xs:complexContent>
      <xs:extension base="baci:CharacteristicComponent">
        <xs:sequence>
	      <xs:element name="frequency" type="baci:ROdouble" />
	      <xs:element name="amplitude" type="baci:ROdouble" />
	      <xs:element name="isLocked" type="baci:ROdouble" />          
        </xs:sequence>
        <xs:attribute name="IP" type="xs:string" use="required"/>
        <xs:attribute name="PORT" type="xs:unsignedInt" use="required"/>
        <xs:attribute name="FREQ_CMD" type="xs:string" use="required"/>        
        <xs:attribute name="FREQ_ANS_OK" type="xs:string" use="required"/>
        <xs:attribute name="AMP_CMD" type="xs:string" use="required"/>
        <xs:attribute name="AMP_ANS_OK" type="xs:string" use="required"/>
        <xs:attribute name="RF_ON_CMD" type="xs:string" use="required"/>
        <xs:attribute name="RF_ON_ANS_OK" type="xs:string" use="required"/>
        <xs:attribute name="RF_OFF_CMD" type="xs:string" use="required"/>
        <xs:attribute name="RF_OFF_ANS_OK" type="xs:string" use="required"/>
        <xs:attribute name="FREQ_QUERY" type="xs:string" use="required"/>
        <xs:attribute name="FREQ_QUERY_NOK" type="xs:string" use="required"/>
        <xs:attribute name="AMP_QUERY" type="xs:string" use="required"/>
        <xs:attribute name="AMP_QUERY_NOK" type="xs:string" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:element name="LocalOscillatorEthernetCommand" type="LocalOscillatorEthCommandType"/>
</xs:schema>
