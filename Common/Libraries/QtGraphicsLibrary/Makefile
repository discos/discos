Makefile: IRA_graphics.pro  /usr/lib/qt-3.3/mkspecs/default/qmake.conf /usr/lib/qt-3.3/lib/libqt-mt.prl
	$(QMAKE) -o Makefile IRA_graphics.pro
qmake: 
	@$(QMAKE) -o Makefile IRA_graphics.pro

