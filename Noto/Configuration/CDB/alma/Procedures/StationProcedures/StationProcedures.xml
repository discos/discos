<Procedure name="setupCCC" args="0">
<body>
	antennaSetup=SEC
	initialize=CCC
	receiversSetup=CCC
	asOn
	device=0
	calOff
	setAttenuation=0,15
	setAttenuation=1,5
</body>	
</Procedure>

