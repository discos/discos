<!-- timeout in microseconds -->
<KQWBandReceiver
    xmlns="urn:schemas-cosylab-com:KQWBandReceiver:1.0" 
    xmlns:baci="urn:schemas-cosylab-com:BACI:1.0" 
    xmlns:cdb="urn:schemas-cosylab-com:CDB:1.0" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    actionThreadStackSize="2048"
    monitoringThreadStackSize="4096"
    DewarIPAddress="192.168.200.181"
    DewarPort="5002"
    LNAIPAddress="192.168.200.182"
    LNAPort="5002"      
    WatchDogResponseTime="10000000"
    WatchDogSleepTime="10000000"
    LNASamplingTime="2500000"
    RepetitionCacheTime="7000000"
    RepetitionExpireTime="10000000"
    ReceiverName="KQWBand"
    DefaultMode="Normal"
    LocalOscillator_K_Instance="RECEIVERS/LO_KBAND"
    LocalOscillator_Q_Instance="RECEIVERS/LO_KQW_Q"
    LocalOscillator_W1_Instance="RECEIVERS/LO_KQW_WLOW"
    LocalOscillator_W2_Instance="RECEIVERS/LO_KQW_WHIGH"
>
    <LO description="Sequence of local oscillator values for each IF chain" />
    <feeds description="Number of feeds of the receiver" />
    <IFs description="Number of Intermediate Frequencies chains coming from each feed" />
    <polarization description="Sequence of values that identifies the polarization of each IF chains" />    
    <initialFrequency description="Sequence of the start frequencies for each IF chain (MHz)" 
                      units="MHz"
                      format="%6.2f"
    />  
    <bandWidth description="Sequence of the band width values for each IF chain (MHz)" 
               units="MHz"
               format="%6.2f"
    />
    <mode description="Mnemonic code that defines a special working mode of the receiver (MHz)" />
    <vacuum description="measure of the vacuum"
            alarm_high_on="5e-3"
            default_value="5e-7"
            alarm_high_off="5e-6"
            alarm_timer_trig="0"
            alarm_fault_family="BACIProperty"
            alarm_fault_member="KQWBandReceiver"
            alarm_level="0"
            default_timer_trig="10"
            min_timer_trig="1"
            min_delta_trig="1e-7"
            units="mBar"
            format="%e"
    />
    <status description="Bit fields that provide the status word of the receiver and component"
            alarm_mask="777"
            alarm_trigger="1"
            alarm_timer_trig="1"
            alarm_fault_family="BACIProperty"
            alarm_fault_member="KQWBandReceiver"
            alarm_level="0"
            default_timer_trig="10"
            min_timer_trig="1"
    />  
    <vdKL description="drain voltage for the LNA left channel, K band"
          units="Volts"
          format="%5.2f"
    />
    <vdKR description="drain voltage for the LNA right channel, K band"
          units="Volts"
          format="%5.2f"
    />
    <vdQL description="drain voltage for the LNA left channel, Q band"
          units="Volts"
          format="%5.2f"
    />

    <vdQR description="drain voltage for the LNA right channel, Q band"
          units="Volts"
          format="%5.2f"
    />

    <vdWL description="drain voltage for the LNA left channel, W band"
          units="Volts"
          format="%5.2f"
    />

    <vdWR description="drain voltage for the LNA right channel, W band"
          units="Volts"
          format="%5.2f"
    />

    <idKL description="drain current for the LNA left channel, K band"
          units="milliAmpere"
          format="%5.2f"
    />

    <idKR description="drain current for the LNA right channel, K band"
          units="milliAmpere"
          format="%5.2f"
    />

    <idQL description="drain current for the LNA left channel, Q band"
  
          units="milliAmpere"
          format="%5.2f"
    />

    <idQR description="drain current for the LNA right channel, Q band"
          units="milliAmpere"
          format="%5.2f"
    />

    <idWL description="drain current for the LNA left channel, Q band"
          units="milliAmpere"
          format="%5.2f"
    />

    <idWR description="drain current for the LNA right channel, W band"

          units="milliAmpere"
          format="%5.2f"
    />

    <vgKL description="gate voltage for the LNA left channel, K band"
          units="Volts"
          format="%5.2f"
    />

    <vgKR description="gate voltage for the LNA right channel, K band"
          units="Volts"
          format="%5.2f"
    />

    <vgQL description="gate voltage for the LNA left channel, Q band"
          units="Volts"
          format="%5.2f"
    />

    <vgQR description="gate voltage for the LNA right channel, Q band"
          units="Volts"
          format="%5.2f"
    />

    <vgWL description="gate voltage for the LNA left channel, W band"
          units="Volts"
          format="%5.2f"
    />

    <vgWR description="gate voltage for the LNA right channel, W band"
          units="Volts"
          format="%5.2f"
    />
    <cryoTemperatureCoolHead description="Cryogenic temperature measured near the cool head"
          alarm_high_on="90.0"
          alarm_high_off="87.0"
          alarm_low_on="45.0"
          alarm_low_off="50.0"
          default_value="60.0"
          alarm_timer_trig="0"
          alarm_fault_family="BACIProperty"
          alarm_fault_member="KQWBandReceiver"
          alarm_level="0"
          default_timer_trig="10"
          min_timer_trig="1"
          min_delta_trig="0.5"
          units="Kelvin"
          format="%5.1f"                                    
    />
    <cryoTemperatureCoolHeadWindow description="Cryogenic temperature measured at cool head window"
          alarm_high_on="90.0"
          alarm_high_off="87.0"
          alarm_low_on="60.0"
          alarm_low_off="62.0"
          default_value="77.0"
          alarm_timer_trig="0"
          alarm_fault_family="BACIProperty"
          alarm_fault_member="KQWBandReceiver"
          alarm_level="0"
          default_timer_trig="10"
          min_timer_trig="1"
          min_delta_trig="0.5"
          units="Kelvin"
          format="%5.1f"                        
    />
    <cryoTemperatureLNA description="Cryogenic temperature measured at the LNA"
          alarm_high_on="30.0"
          alarm_high_off="27.0"
          alarm_low_on="13.5"
          alarm_low_off="14.0"
          default_value="16.0"
          alarm_timer_trig="1.0"
          alarm_fault_family="BACIProperty"
          alarm_fault_member="KQWBandReceiver"
          alarm_level="0"
          default_timer_trig="1.0"
          min_timer_trig="1"
          min_delta_trig="0.5"
          units="Kelvin"
          format="%5.1f"                                    
    />
    <cryoTemperatureLNAWindow description="Cryogenic temperature measured at the LNA window"
          alarm_high_on="30.0"
          alarm_high_off="27.0"
          alarm_low_on="13.5"
          alarm_low_off="14.0"
          default_value="16.0"
          alarm_timer_trig="0"
          alarm_fault_family="BACIProperty"
          alarm_fault_member="KQWBandReceiver"
          alarm_level="0"
          default_timer_trig="10"
          min_timer_trig="1"
          min_delta_trig="0.5"
          units="Kelvin"
          format="%5.1f"                                                
    />
    <environmentTemperature description="temperature of the room in which the receiver is installed"
          alarm_high_on="47.0"
          alarm_high_off="44.0"
          alarm_low_on="0.0"
          alarm_low_off="1.0"
          alarm_fault_family="BACIProperty"
          alarm_fault_member="KQWBandReceiver"
          default_value="33.0"
          alarm_timer_trig="1.0"
          alarm_level="0"
          default_timer_trig="10"
          min_timer_trig="1"
          min_delta_trig="0.5"
          units="Celsius degree"
          format="%4.1f"                                    
    />
    <receiverStatus description="Overall status of the receiver"/>

    <receiverName description="Name of the receiver"/>

</KQWBandReceiver>
