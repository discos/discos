<!-- L Band polarization, L band filter, P band polarization, P band filter

     L1L1:
     - Linear polarization of the L band feed
     - L Band filter n.1 - 1300-1800 MHz, without filter (all band)
     - Linear polarization of the P band feed
     - P Band filter n.1 - 305-410 MHz, without filter (all band) 
-->
<SRTLPBandReceiverModeSetup xmlns="urn:schemas-cosylab-com:SRTLPBandReceiverModeSetup:1.0" 
	xmlns:baci="urn:schemas-cosylab-com:BACI:1.0"
	xmlns:cdb="urn:schemas-cosylab-com:CDB:1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	
	Mode="L1L1"
    LBandRFMin="1300.0 1300.0"
    LBandRFMax="1800.0 1800.0"
    PBandRFMin="305.0 305.0"
    PBandRFMax="410.0 410.0"
    IFs="2"
    LBandIFMin="100.0 100.0"
    PBandIFMin="100.0 100.0"
    Feeds="2" 
    LBandPolarization="V H"
    PBandPolarization="V H"
/>
